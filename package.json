      "ie9",
      "ie10",
      "ff/20",
      "ff/23",
      "ff/nightly",
      "chrome/29",
      "chrome/canary",
      "opera/15",
      "opera/next",
      "safari/6.0"
    ],
    "harness": "mocha",
    "files": [
      "test/json*.js"
    ]
  },
  "engines": {
    "node": ">= 0.10.x"
  }
}
